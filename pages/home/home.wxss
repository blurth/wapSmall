@import "../tpls/base.wxss";
@import "../tpls/products/products-tpl.wxss";
.swiper{
  height: 400rpx;
  width: 100%;
}
.swiper-box{
  overflow-x: hidden;
}
.banner-item{
  height: 100%;
  width: 100%;
}
.banner-item image{
  height: 100%;
  width: 100%;
}

.home-main-header{
  display: flex;
  height: 60rpx;
  color: #AB956D;
  background-color: #fff;
  font-size: 32rpx;
  align-items: center;
  justify-content: center;
  margin: 20rpx 0;
}

.theme-box{
  display: flex;
  flex-wrap: wrap;
  align-content: space-between;
  width: 750rpx;
  margin-top: 10rpx;


}
.theme-item{
  display: flex;
  margin-bottom: 10rpx;
  width: 25%;
  justify-content: center;
  position: relative;
  box-sizing: border-box;
}
/* .theme-item:first-child{
  border-right:4rpx solid #fff;
} */
.theme-item.big{
  width: 100%;
}
.theme-item.big .theme-icon view{
  margin: 0 auto;
  width: 150rpx;
  height: 60rpx;
  border-radius: 60rpx;
  line-height: 60rpx;
  border:1rpx solid #fff;
  text-align: center;
  font-size: 24rpx;
}
.theme-item image{
 height: 150rpx;
  width: 90%;
}
.theme-icon{
  margin-top:10rpx;
  height: 48rpx;
}
.theme-icon image{
  height: 48rpx;
  width: 48rpx;
}

.fen-q{
  padding: 0;
  background-color: #fff;
  border: 0px;
}

.fen-q::after{
  border:0; 
}

.case-item image{
  width: 100%;
}

.case-item{
  width: 750rpx;
  height:600rpx;
  border-bottom: 20rpx solid #f2f2f2;
  border-top: 20rpx solid #f2f2f2;
  overflow: hidden;
}
.ca-top{
  height: 90rpx;
  position: relative;
  text-align: center;
}
.ca-top text{
  text-align:center;
  height:90rpx;
  line-height:90rpx;
  color:#333;
  font-size:32rpx;
}
.ca-top image{
width:36rpx;
height:36rpx;
margin-bottom:-6rpx;
margin-left:10rpx;

}

.cell{
  float:left;
width:373rpx;
height:250rpx;
position:relative;

}
.cell image{
  height:250rpx;
}
.cell-img{
  position: relative;
}
.cell-text{
  position:absolute;
top:60rpx;
left:10rpx;
color:#8d8d8d;
font-size:30rpx;

}
.describe{
  height:50rpx;
margin-left:5rpx;
position:absolute;
z-index:999;

}
.no1{
    border-top: 1rpx solid #f2f2f2;
    border-right: 0.5rpx solid #f2f2f2;
    border-bottom: 0.5rpx solid #f2f2f2;
}
.no2{
    border-top: 1rpx solid #f2f2f2;
    border-left: 0.5rpx solid #f2f2f2;
    border-bottom: 0.5rpx solid #f2f2f2;
}
.no3{
    border-right: 0.5rpx solid #f2f2f2;
    border-top: 0.5rpx solid #f2f2f2;
}
.no4{
    border-left: 0.5rpx solid #f2f2f2;
    border-top: 0.5rpx solid #f2f2f2;
}

.kanjia-container {
    width: 100%;
    height:780rpx;
    border-bottom: 20rpx solid #f5f5f5;
}

.kanjia-title {
    text-align: center;
    height: 90rpx;
    line-height: 90rpx;
    color: #333;
    font-size: 32rpx;
}

.kanjia-title image {
    width: 36rpx;
    height: 36rpx;
    margin-bottom: -6rpx;
    margin-left: 10rpx;
}

.kanjia-list {
    float: left;
    height: 230rpx;
    width: 100%;
    border-top: 1px solid #f5f5f5;
}

.kanjia-list-image {
    float: left;
    margin-left: 20rpx;
    margin-top: 16rpx;
}

.kanjia-list-image image {
    width: 200rpx;
    height: 200rpx;
    border-radius: 6rpx;
}

.kanjia-list-info {
    float: left;
    width: 65%;
    margin-left: 20rpx;
    height: 250rpx;
    overflow: hidden;
}

.kanjia-list-info-title {
    font-size: 30rpx;
    margin-top: 20rpx;
    color: #333;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.kanjia-list-info-info {
    font-size: 24rpx;
    color: #999;
    margin-top: 10rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.kanjia-list-info-money {
    margin-top: 30rpx;
}

.kanjia-list-info-money-a {
    display: inline-block;
    width: 33.33%;
    font-size: 26rpx;
    color: #999;
}

.kanjia-list-info-money-a text {
    display: block;
}

.kanjia-list-info-money-a .a {
    color: #ab2b2b;
    font-size: 30rpx;
}

.kanjia-list-info-money-a .b {
    color: #333;
}
